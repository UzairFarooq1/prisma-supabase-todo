generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model todo{
  id Int @id @default(autoincrement())
  title String
  userId    Int @default(1)
  user      User     @relation(fields: [userId], references: [id])
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  todos    todo[]
}